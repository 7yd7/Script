-- مخصص بلاك ليست
local player = game.Players.localPlayer
-- اعطاء لاعبين بلاك ليست
if player.name == "اسم لاعب" or player.name == "اسم لاعب اخر" then

-- اللوج بلاك ليست
local url = "https://raw.githubusercontent.com/7yd7/7yd7/main/log-blacklist" -- اللوج
loadstring(game:HttpGet(url))()

-- طرد لاعب
player:Kick("بلاك ليست (سوف يتم عمل ريجوين لك بعد كم ثانيه)")

-- rejoin
wait(2)
game:GetService("TeleportService"):Teleport(game.PlaceId, game:GetService("Players").LocalPlayer)

wait(999)
end


-- مهم جدا
workspace.FallenPartsDestroyHeight = -50000
local placeId = game.PlaceId

-- اللوج ( غير مجبور اذا كنت تريده او لا او مسحه )
if placeId == 13600599498 or placeId == 11984550654 or placeId == 10598963932 or placeId == 10659321502 or placeId == 13122706381 or placeId == 13618858186 or placeId == 4954752502 or placeId == 2965497514 then
-- تشغيل سكربت + دخول لاعب
